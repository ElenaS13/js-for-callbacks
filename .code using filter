var animals = [
  { name: "Waffles", type: "dog", age: 12 },
  { name: "Spelunky", type: "dog", age: 4 },
  { name: "Hank", type: "dog", age: 11 },
];
var oldDogNames = 
  animals
    .filter(function(animal) {
      return animal.age > 10 && animal.type === "dog";
    })
    .map(function(animal) {
      return animal.name;
    });
console.log(oldDogNames);
